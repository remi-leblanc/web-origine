{
	"name": "weborigine",
	"displayName": "WebOrigine",
	"version": "0.0.0",
	"description": "A basic Plasmo extension.",
	"author": "Kana Work",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.2.1",
		"@fortawesome/free-regular-svg-icons": "^6.2.1",
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"animejs": "^3.2.1",
		"axios": "^1.1.3",
		"plasmo": "0.58.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "^4.9.3"
	},
	"devDependencies": {
		"@plasmohq/prettier-plugin-sort-imports": "3.5.2",
		"@types/chrome": "0.0.197",
		"@types/node": "18.8.3",
		"@types/react": "18.0.21",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.18",
		"prettier": "2.7.1",
		"tailwindcss": "^3.2.2"
	},
	"manifest": {
		"web_accessible_resources": [
			{
				"resources": [
					"assets/images/regions/*.jpg"
				],
				"matches": [
					"https://www.plasmo.com/*"
				]
			}
		],
		"host_permissions": [
			"https://*/*"
		],
		"permissions": [
			"tabs"
		]
	}
}